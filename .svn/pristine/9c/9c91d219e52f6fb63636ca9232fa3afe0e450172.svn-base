@model IEnumerable<Mapping_Solution.Models.MapDetagSubarnMasterDetail>

<table class="table small border" id="tblMapDetagSubarnMaster">
    <thead>
        <tr class="bg-primary text-white">
            <th> </th>
            <th scope="col">ARN_NO</th>
            <th scope="col">BRANCH_CODE</th>
            <th scope="col">ARN_NAME</th>
            <th scope="col">VALID_FROM</th>
            <th scope="col">VALID_UPTO</th>
            <!-- <th scope="col">ACTION</th> -->
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td><input type="checkbox" class="form-check-input" /></td>

                <td>
                    @Html.DisplayFor(modelItem => item.arn_no)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.branch_code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.arn_name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valid_from)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valid_upto)
                </td>
                @*<td>
                        <select class="form-select">
                            <option>Select</option>
                            <option>Edit</option>
                            <option>Save</option>
                            <option>Delete</option>
                        </select>
                    </td>*@

            </tr>
        }
    </tbody>
</table>

