@model IEnumerable<Mapping_Solution.Models.UfcMasterDetails>

<table class="table small border" id="tblUFCMaster">
    <thead>
        <tr class="bg-primary text-white">
            <th> </th>
            <th scope="col">SAP_UFC_CODE</th>
            <th scope="col">UFC_CODE</th>
            <th scope="col">UFC_NAME</th>
            <th scope="col">UFC_START_DATE</th>
            <th scope="col">REGION_CODE</th>
            <th scope="col">REGION_CD_CHNL</th>
            <th scope="col">ZONE</th>
            <th scope="col">STATE</th>
            <th scope="col">IMC_CITY</th>
            <th scope="col">REGION_SAP_CODE</th>
            <th scope="col">ZONE_SAP_CODE</th>
            <th scope="col">LATITUDE</th>
            <th scope="col">LONGITUDE</th>
            <th scope="col">UFC_ADDRESS</th>
            <th scope="col">PINCODE</th>
            <th scope="col">UFC_TYPE</th>
            <th scope="col">VALID_FROM</th>
            <th scope="col">VALID_UPTO</th>
            <!-- <th scope="col">ACTION</th> -->
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
        <tr>
            <td><input type="checkbox" class="form-check-input"></td>
            <td>
                @Html.DisplayFor(modelItem => item.sap_ufc_code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ufc_code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ufc_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ufc_start_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.region_code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.region_cd_chnl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.zone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.state)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.imc_city)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.region_sap_code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.zone_sap_code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.latitude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.longitude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ufc_address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.pincode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ufc_type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.valid_from)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.valid_upto)
            </td>
            @*<td>
            <select class="form-select">
                <option>Select</option>
                <option>Edit</option>
                <option>Save</option>
                <option>Delete</option>
            </select>
        </td>*@
        </tr>
        }
    </tbody>
</table>
