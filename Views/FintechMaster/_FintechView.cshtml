@model IEnumerable<Mapping_Solution.Models.FintechMasterDetails>

<table class="table small border" id="tblFintechMaster">
    <thead>
        <tr class="bg-primary text-white">
            <th> </th>
            <!-- <th scope="col">SRNO</th> -->
            <th scope="col">ARN_RIA_CODE</th>
            <!-- <th scope="col">DISTRIBUTORSNAME</th> -->
            <th scope="col">MULTICITY_FLAG_AM</th>
            <th scope="col">CHANNEL</th>
            <th scope="col">FAMILY_ARN_CODE</th>
            <th scope="col">FAMILY_ARN_NAME</th>
            <th scope="col">GROUPTYPE</th>
            <th scope="col">MFDGROUP</th>
            <th scope="col">ARNTYPE</th>
            <th scope="col">STRATEGICTEAMRM</th>
            <th scope="col">FINTECH_TYPE</th>
            <th scope="col">FINTECH_RMCODE</th>
            <th scope="col">VALID_FROM</th>
            <th scope="col">VALID_UPTO</th>
            <!-- <th scope="col">ACTION</th> -->
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td><input type="checkbox" class="form-check-input"></td>
                <td>
                    @Html.DisplayFor(modelItem => item.arn_ria_code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.multicity_flag_am)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.channel)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.family_arn_code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.family_arn_name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.group_type)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.mfd_group)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.arn_type)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.strategic_team_rm)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.fintech_type)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.fintech_rm_code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valid_from)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valid_upto)
                </td>
                @*<td>
                        <select class="form-select">
                            <option>Select</option>
                            <option>Edit</option>
                            <option>Save</option>
                            <option>Delete</option>
                        </select>
                    </td>*@
            </tr>
        }
    </tbody>
</table>
