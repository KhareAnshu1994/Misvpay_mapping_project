@model IEnumerable<Mapping_Solution.Models.SchemeCategoryDetail>

<table class="table small border" id="tblSchemeCatMaster">
    <thead>
        <tr class="bg-primary text-white">
            <th> </th>
            <th scope="col">SCHEME_CODE</th>
            <!-- <th scope="col">SCHEME_CATEGORY_CODE</th> not required -->
            <th scope="col">SUB_FUND_CODE</th>
            <th scope="col">KARVY_SCHEME_CODE</th>
            <th scope="col">PLAN</th>
            <th scope="col">SCHEME_NAME</th>
            <th scope="col">NEW_SEBI_SCHEME_TYPE</th>
            <th scope="col">NEW_SEBI_CATEGORY</th>
            <th scope="col">NEW_SEBI_SUB_CATEGORY</th>
            <th scope="col">CAMS_CATEGORY</th>
            <th scope="col">AMFI_CATEGORY</th>
            <th scope="col">MIS_CATEGORY</th>
            <th scope="col">MIS_SUB_CATEGORY</th>
            <th scope="col">VALID_FROM</th>
            <th scope="col">VALID_UPTO</th>
            <!-- <th scope="col">ACTION</th> -->
        </tr>
    </thead>
    <tbody>


        @foreach (var item in Model)
        {
        <tr>
            <td><input type="checkbox" class="form-check-input" /></td>
            <td>
                @Html.DisplayFor(modelItem => item.scheme_code)
            </td>
            @* <td>
            @Html.DisplayFor(modelItem => item.scheme_category_code)
        </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.sub_fund_code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.karvy_scheme_code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.plan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.scheme_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.new_sebi_scheme_type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.new_sebi_category)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.new_sebi_sub_category)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.cams_category)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.amfi_category)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.mis_category)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.mis_sub_category)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.valid_from)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.valid_upto)
            </td>

            @* <td>
            <select class="form-select">
                <option>Select</option>
                <option>Edit</option>
                <option>Save</option>
                <option>Delete</option>
            </select>
        </td>*@
            @*<td>
            @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
            @Html.ActionLink("Details", "Details", new { id = item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.id })
        </td>*@
        </tr>
        }

    </tbody>
</table>

