@model IEnumerable<Mapping_Solution.Models.RiaDetail>

<table class="table small border" id="tblRiaMaster">
    <thead>
        <tr class="bg-primary text-white">
            <th> </th>
            <th scope="col">RIACODE</th>
            <th scope="col">ARN_CODE</th>
            <th scope="col">VALID_FROM</th>
            <th scope="col">VALID_UPTO</th>
            <th scope="col">CHANNEL_CODE</th>
            <th scope="col">MULTICITY</th>
            <th scope="col">RIA_NAME</th>
            <th scope="col">ARN_NAME</th>
            <!-- <th scope="col">ACTION</th> -->
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td><input type="checkbox" /></td>
                <td>
                    @Html.DisplayFor(modelItem => item.riacode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.arn_code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valid_from)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valid_upto)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.channel_code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.multicity)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ria_name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.arn_name)
                </td>
                @* <td>
                        <select class="form-select">
                            <option>Select</option>
                            <option>Edit</option>
                            <option>Save</option>
                            <option>Delete</option>
                        </select>
                    </td>*@

                @*<td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                        @Html.ActionLink("Details", "Details", new { id = item.id }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.id })
                    </td>*@
            </tr>
        }
    </tbody>
</table>

