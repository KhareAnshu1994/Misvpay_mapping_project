                        <option value="Y">Working</option>
                        <option selected="selected" value="N">Not Working</option>
                    </select>

                </div>
                <div class="form-group col-md-4 mb-3">
                    <label> Employee Role </label>
                    <select name="ddlEmpRole" id="ddlEmpRole" class="form-select w-100">
