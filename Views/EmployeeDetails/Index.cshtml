                        <option value="Region Head">Region Head</option>
                        <option value="Chief Manager">Chief Manager</option>
                        <option value="Channel Head">Channel Head</option>
                    </select>
                </div>
                <div class="form-group col-md-4 mb-3">
                    <label> Location/UFC </label>
                    <select name="ddlLocation" id="ddlLocation" class="form-select w-100">
                        <option selected="selected" value="Select">Select</option>
                    </select>
                </div>
                <div class="col-md-12 mt-3">
