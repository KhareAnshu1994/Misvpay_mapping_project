@model IEnumerable<Mapping_Solution.Models.SubbrokerMasterDetails>



<table class="table small border" id="tblSubbrokerMaster">
    <thead>
        <tr class="bg-primary text-white">
            <th> </th>
            <th scope="col">ARNNO</th>
            <th scope="col">SUBBROKER_CODE</th>
            <th scope="col">CHANNEL_CODE</th>
            <th scope="col">REGION_CODE</th>
            <th scope="col">UFC_CODE</th>
            @*<th scope="col">RMCODE</th>*@
            @*<th scope="col">PINCODE</th> no required*@
            <th scope="col">KRV_CODE</th>
            <th scope="col">VALID_FROM</th>
            <th scope="col">VALID_UPTO</th>
            <th scope="col">LST_UPDDT</th>
            <th scope="col">ZONE</th>
            @*<th scope="col">EUIN_BCODE</th> no required*@
            <th scope="col">SAP_UFC_CODE</th>
            <th scope="col">UFC_LOCATION</th>
            @*<th scope="col">SAP_REGION_CODE</th>
                <th scope="col">SAP_ZONE_CODE</th> no display*@
            <th scope="col">CREATION_DT</th>
            @*<th scope="col">ADDRESS</th>
                <th scope="col">EMAIL_ID</th>
                <th scope="col">PHONE_NO</th>*@
            <!-- <th scope="col">ACTION</th> -->
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td><input type="checkbox" class="form-check-input"></td>
                <td>
                    @Html.DisplayFor(modelItem => item.arn_no)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.subbroker_code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.channel_code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.region_code)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.branch_code)
                    </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.ufc_code)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.rm_code)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.pin_code)
                    </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.krv_code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valid_from)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valid_upto)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.lst_upddt)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.zone)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.euin_bcode)
                    </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.sap_ufc_code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ufc_location)
                </td>
                @*<td>
                    @Html.DisplayFor(modelItem => item.sap_region_code)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sap_zone_code)
                </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.creation_date)
                </td>
                @*<td>
                        <select class="form-select">
                            <option>Select</option>
                            <option>Edit</option>
                            <option>Save</option>
                            <option>Delete</option>
                        </select>
                    </td>*@
            </tr>
        }
    </tbody>
</table>